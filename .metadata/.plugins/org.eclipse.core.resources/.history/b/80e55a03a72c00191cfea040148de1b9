package dlgModerator;

public class DlgModerator 
{private static SpielerVerwalter w_spielerListe = new SpielerVerwalter();
public static void main(String[] args) 
	{w_spielerListe.SpielerListe_erzeuge();
	w_spielerListe.FragenRunde_starten();
	for (int i = w_spielerListe.z_spieler-1 ; i>=0; i--)
		{System.out.print(w_spielerListe.getSpieler(i).getName()+ "  ");
		for (int j = 0 ; j<2 ;j++)
			{System.out.print(w_spielerListe.getProtokoll()[i][j] + "  ");	
			}
		System.out.println();
		}	
	}

	
}
